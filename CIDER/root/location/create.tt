[% page.title = 'Create location ' _ barcode %]

<h2>[% page.title %]</h2>

<div class="required_message">
     <p>Fields marked with a '*' are required</p>
</div>


[% IF return_uri %]
<div class="form_error_message">
<p>
No location with barcode <em>[% barcode %]</em> exists.  Please create
the location below, after which the item <em>[% title %]</em>
will be [% action %]d.
</p>
</div>
[% END %]

[% form %]
